<ModelBehaviors>
		<Include ModelBehaviorFile="Asobo\Generic\FX.xml"/>
		<Include ModelBehaviorFile="Asobo\Exterior.xml"/>
		<Include ModelBehaviorFile="AIG\Animations.xml"/>
		<Include ModelBehaviorFile="AIG\AnimationsComplex.xml"/>
		<Include ModelBehaviorFile="AIG\EngineEffects.xml"/>
		<Include ModelBehaviorFile="AIG\LandingFX.xml"/>
	
<Template Name="HTAI_Beechjet_Common">

	<Component ID="Landing_Gear">
		<UseTemplate Name="AIG_Gear_Center_Template">
			<ANIM_NAME>c_gear</ANIM_NAME>
			<ANIM_SIMVAR_SCALE>2</ANIM_SIMVAR_SCALE>
			<ANIM_SIMVAR_UNITS>Keyframe</ANIM_SIMVAR_UNITS>
		</UseTemplate>
		<UseTemplate Name="AIG_Gear_Left_Template">
			<ANIM_NAME>l_gear</ANIM_NAME>
			<ANIM_SIMVAR_SCALE>2</ANIM_SIMVAR_SCALE>
			<ANIM_SIMVAR_UNITS>Keyframe</ANIM_SIMVAR_UNITS>
		</UseTemplate>
		<UseTemplate Name="AIG_Gear_Right_Template">
			<ANIM_NAME>r_gear</ANIM_NAME>
			<ANIM_SIMVAR_SCALE>2</ANIM_SIMVAR_SCALE>
			<ANIM_SIMVAR_UNITS>Keyframe</ANIM_SIMVAR_UNITS>
		</UseTemplate>
	</Component>

	<Component ID="Wheels">
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>c_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>0</WHEEL_ID>
			<RATE>10</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>l_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>1</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>r_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>2</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
	</Component>
	
	<Component ID="Nosewheel_Steering">
		<UseTemplate Name="AIG_Nosewheel_Steering">
			<ANIM_NAME>custom_anim_C_WHEEL_LR</ANIM_NAME>
			<ANIM_SHARPNESS>50</ANIM_SHARPNESS>
		</UseTemplate>
	</Component>
	
	<Component ID="Spoilers">
		<UseTemplate Name="AIG_Spoiler_L_key"/>
		<UseTemplate Name="AIG_Spoiler_R_key"/>
	</Component>
	
	<Component ID="Engines">
		<UseTemplate Name="AIG_Engine_Jet">
			<ANIM_NAME>engine0</ANIM_NAME>
			<ENGINE_ID>1</ENGINE_ID>
		</UseTemplate>
		<UseTemplate Name="AIG_Engine_Jet">
			<ANIM_NAME>engine1</ANIM_NAME>
			<ENGINE_ID>2</ENGINE_ID>
		</UseTemplate>
	</Component>

	<Component ID="Flaps">
		<UseTemplate Name="AIG_Flaps">
			<FLAP_TO_DEGREES>35</FLAP_TO_DEGREES>
			<FLAP1_SPEED>160</FLAP1_SPEED>
			<FLAP1_DEGREES>100</FLAP1_DEGREES>
			<FLAP2_SPEED>180</FLAP2_SPEED>
			<FLAP2_DEGREES>50</FLAP2_DEGREES>
			<FLAP3_SPEED>200</FLAP3_SPEED>
			<FLAP3_DEGREES>35</FLAP3_DEGREES>
		</UseTemplate>
	</Component>
	
	<Component ID="AnimatedLights">
		<UseTemplate Name="AIG_AnimatedLandingTaxiLights">
			<ANIM_NAME>beechjet_lights_v2</ANIM_NAME>
		</UseTemplate>
	</Component>
	
	<Component ID="Effects">
		<UseTemplate Name="AIG_TwinEngineJet_Effects"/>
		<UseTemplate Name="AIG_LandingFX"/>
	</Component>

</Template>

<Template Name="HTAI_Beechjet_Reverser">
	<Component ID="Reverse_Thrust">
		<UseTemplate Name="AIG_Reverse_Thrust">
			<ANIM_NAME>custom_anim_REV_THRUST</ANIM_NAME>
		</UseTemplate>
	</Component>
</Template>

</ModelBehaviors>