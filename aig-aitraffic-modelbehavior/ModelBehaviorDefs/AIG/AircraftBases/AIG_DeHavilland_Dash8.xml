<ModelBehaviors>
		<Include ModelBehaviorFile="Asobo\Generic\FX.xml"/>
		<Include ModelBehaviorFile="Asobo\Exterior.xml"/>
		<Include ModelBehaviorFile="AIG\Animations.xml"/>
		<Include ModelBehaviorFile="AIG\AnimationsComplex.xml"/>
		<Include ModelBehaviorFile="AIG\EngineEffects.xml"/>
		<Include ModelBehaviorFile="AIG\LandingFX.xml"/>
	
<Template Name="AIG_DeHavilland_Dash8_Common">

	<Component ID="Landing_Gear">
		<UseTemplate Name="AIG_Gear_Center_Template"/>
		<UseTemplate Name="AIG_Gear_Left_Template"/>
		<UseTemplate Name="AIG_Gear_Right_Template"/>
	</Component>
	
	<Component ID="Wheels">
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>c_tire</ANIM_NAME>
			<WHEEL_ID>0</WHEEL_ID>
			<RATE>10</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>l_tire</ANIM_NAME>
			<WHEEL_ID>1</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>r_tire</ANIM_NAME>
			<WHEEL_ID>2</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
	</Component>

	<Component ID="Nosewheel_Steering">
		<UseTemplate Name="AIG_Nosewheel_Steering">
			<ANIM_NAME>custom_anim_C_WHEEL_LR</ANIM_NAME>
			<ANIM_SHARPNESS>30</ANIM_SHARPNESS>
		</UseTemplate>
	</Component>

	<Component ID="Rudder">
		<UseTemplate Name="AIG_Rudder_Deflection"/>
	</Component>

	<Component ID="Elevator">
		<UseTemplate Name="AIG_Dash8_Elevator">
			<ANIM_NAME>elevator</ANIM_NAME>
		</UseTemplate>
	</Component>

	<Component ID="Engines">
		<UseTemplate Name="AIG_Engine_Propeller">
			<ANIM_NAME>engine0</ANIM_NAME>
			<ENGINE_ID>1</ENGINE_ID>
		</UseTemplate>
		<UseTemplate Name="AIG_Engine_Propeller">
			<ANIM_NAME>engine1</ANIM_NAME>
			<ENGINE_ID>2</ENGINE_ID>
		</UseTemplate>
	</Component>

	<Component ID="Engines_Visibility">
		<Component ID="Prop1_Still">
			<UseTemplate Name="AIG_Engine_Propeller_Still">
				<ENGINE_ID>1</ENGINE_ID>
				<NODE_ID>prop1_still</NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="Prop1_Slow">
			<UseTemplate Name="AIG_Engine_Propeller_Slow">
				<ENGINE_ID>1</ENGINE_ID>
				<NODE_ID>prop1_slow</NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Engine_Propeller_Slow">
				<ENGINE_ID>1</ENGINE_ID>
				<NODE_ID>prop1_spinner_slow</NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="Prop1_Blurred">
			<UseTemplate Name="AIG_Engine_Propeller_Blurred">
				<ENGINE_ID>1</ENGINE_ID>
				<NODE_ID>prop1_blur</NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Engine_Propeller_Blurred">
				<ENGINE_ID>1</ENGINE_ID>
				<NODE_ID>prop1_spinner_blur</NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="Prop2_Still">
			<UseTemplate Name="AIG_Engine_Propeller_Still">
				<ENGINE_ID>2</ENGINE_ID>
				<NODE_ID>prop2_still</NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="Prop2_Slow">
			<UseTemplate Name="AIG_Engine_Propeller_Slow">
				<ENGINE_ID>2</ENGINE_ID>
				<NODE_ID>prop2_slow</NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Engine_Propeller_Slow">
				<ENGINE_ID>2</ENGINE_ID>
				<NODE_ID>prop2_spinner_slow</NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="Prop2_Blurred">
			<UseTemplate Name="AIG_Engine_Propeller_Blurred">
				<ENGINE_ID>2</ENGINE_ID>
				<NODE_ID>prop2_blur</NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Engine_Propeller_Blurred">
				<ENGINE_ID>2</ENGINE_ID>
				<NODE_ID>prop2_spinner_blur</NODE_ID>
			</UseTemplate>
		</Component>
	</Component>

	<Component ID="Flaps">
		<UseTemplate Name="AIG_Flaps">
			<ANIM_NAME>Flaps</ANIM_NAME>
			<FLAP_TO_DEGREES>50</FLAP_TO_DEGREES>
			<FLAP1_SPEED>150</FLAP1_SPEED>
			<FLAP1_DEGREES>100</FLAP1_DEGREES>
			<FLAP2_SPEED>165</FLAP2_SPEED>
			<FLAP2_DEGREES>50</FLAP2_DEGREES>
			<FLAP3_SPEED>180</FLAP3_SPEED>
			<FLAP3_DEGREES>30</FLAP3_DEGREES>
		</UseTemplate>
    </Component>

	<Component ID="Light_Visibility">
		<Component ID="Logo_Lights">
			<UseTemplate Name="AIG_Logolight_Visibility">
				<NODE_ID>LIGHT_Logo_left</NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Logolight_Visibility">
				<NODE_ID>LIGHT_Logo_right</NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="NoseGear_Landing_Lights">
			<UseTemplate Name="AIG_Landinglight_NoseGear_Visibility">
				<NODE_ID>LIGHT_TAXI</NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="Landing_Lightsplash">
			<UseTemplate Name="AIG_Dash8_Landing_Lightsplash">
				<NODE_ID>prop_landinglight</NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="LIGHT_Beacon_vis" Node="LIGHT_Beacon">
			<UseTemplate Name="ASOBO_GT_Visibility">
				<VISIBILITY_CODE>(A:LIGHT STROBE,bool) 1 == if{ 0 } els{ 1 }</VISIBILITY_CODE>
			</UseTemplate>
		</Component>
	</Component>
		
	<Component ID="Effects">
		<UseTemplate Name="AIG_TwinEngineTurboprop_Effects"/>
		<UseTemplate Name="AIG_APU_Heat"/>
		<UseTemplate Name="AIG_LandingFX"/>
	</Component>
	
	<Component ID="Fuselage_DimCabin">
		<UseTemplate Name="AIG_Fuselage_DimCabin">
			<NODE_ID>fuse</NODE_ID>
			<TIME_START>26100</TIME_START>
			<TIME_END>80100</TIME_END>
		</UseTemplate>
	</Component>

</Template>

<Template Name="AIG_DeHavilland_Dash8-100_Spoilers">
	<Component ID="Spoilers">
		<UseTemplate Name="AIG_Dash8-100_Spoiler_L"/>
		<UseTemplate Name="AIG_Dash8-100_Spoiler_R"/>
	</Component>
</Template>

<Template Name="AIG_DeHavilland_Dash8-200-300_Spoilers">
	<Component ID="Spoilers">
		<UseTemplate Name="AIG_Dash8-200-300_Spoiler_L"/>
		<UseTemplate Name="AIG_Dash8-200-300_Spoiler_R"/>
	</Component>
</Template>

<Template Name="AIG_DeHavilland_Dash8-400_Spoilers">
	<Component ID="Spoilers">
		<UseTemplate Name="AIG_Dash8-400_Spoiler_L"/>
		<UseTemplate Name="AIG_Dash8-400_Spoiler_R"/>
	</Component>
</Template>

</ModelBehaviors>