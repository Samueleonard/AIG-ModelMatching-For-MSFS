<ModelBehaviors>
		<Include ModelBehaviorFile="Asobo\Generic\FX.xml"/>
		<Include ModelBehaviorFile="Asobo\Exterior.xml"/>
		<Include ModelBehaviorFile="AIG\Animations.xml"/>
		<Include ModelBehaviorFile="AIG\AnimationsComplex.xml"/>
		<Include ModelBehaviorFile="AIG\EngineEffects.xml"/>
		<Include ModelBehaviorFile="AIG\LandingFX.xml"/>
	
<Template Name="HTAI_Beechcraft_C99_Common">

	<Component ID="Landing_Gear">
		<UseTemplate Name="AIG_Gear_Center_Template">
			<ANIM_NAME>c_gear</ANIM_NAME>
			<ANIM_SIMVAR_SCALE>2</ANIM_SIMVAR_SCALE>
			<ANIM_SIMVAR_UNITS>Keyframe</ANIM_SIMVAR_UNITS>
		</UseTemplate>
		<UseTemplate Name="AIG_Gear_Left_Template">
			<ANIM_NAME>l_gear</ANIM_NAME>
			<ANIM_SIMVAR_SCALE>2</ANIM_SIMVAR_SCALE>
			<ANIM_SIMVAR_UNITS>Keyframe</ANIM_SIMVAR_UNITS>
		</UseTemplate>
		<UseTemplate Name="AIG_Gear_Right_Template">
			<ANIM_NAME>r_gear</ANIM_NAME>
			<ANIM_SIMVAR_SCALE>2</ANIM_SIMVAR_SCALE>
			<ANIM_SIMVAR_UNITS>Keyframe</ANIM_SIMVAR_UNITS>
		</UseTemplate>
	</Component>

	<Component ID="Wheels">
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>c_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>0</WHEEL_ID>
			<RATE>10</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>l_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>1</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>r_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>2</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
	</Component>
	
	<Component ID="Nosewheel_Steering">
		<UseTemplate Name="AIG_Nosewheel_Steering">
			<ANIM_NAME>custom_anim_C_WHEEL_LR</ANIM_NAME>
			<ANIM_SHARPNESS>50</ANIM_SHARPNESS>
		</UseTemplate>
	</Component>

	<Component ID="Engines">
		<UseTemplate Name="AIG_Engine_Propeller">
			<ANIM_NAME>engine0</ANIM_NAME>
			<ENGINE_ID>1</ENGINE_ID>
		</UseTemplate>
		<UseTemplate Name="AIG_Engine_Propeller">
			<ANIM_NAME>engine1</ANIM_NAME>
			<ENGINE_ID>2</ENGINE_ID>
		</UseTemplate>
	</Component>

	<Component ID="Engine_Visibility">
		<Component ID="Engine1">
			<Parameters Type="Override">
				<ENGINE_ID>1</ENGINE_ID>
			</Parameters>
			<UseTemplate Name="AIG_Engine_Propeller_Visibility_Simple_Template">
				<ID>1</ID>
				<STILL_NODE_ID>32</STILL_NODE_ID>
				<BLURRED_NODE_ID>50</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Engine_Propeller_Visibility_Simple_Template">
				<ID>2</ID>
				<STILL_NODE_ID>33</STILL_NODE_ID>
				<BLURRED_NODE_ID>51</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Engine_Propeller_Visibility_Simple_Template">
				<ID>3</ID>
				<STILL_NODE_ID>34</STILL_NODE_ID>
				<BLURRED_NODE_ID>52</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Engine_Propeller_Visibility_Simple_Template">
				<ID>4</ID>
				<STILL_NODE_ID>35</STILL_NODE_ID>
				<BLURRED_NODE_ID>53</BLURRED_NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="Engine2">
			<Parameters Type="Override">
				<ENGINE_ID>2</ENGINE_ID>
			</Parameters>
			<UseTemplate Name="AIG_Engine_Propeller_Visibility_Simple_Template">
				<ID>1</ID>
				<STILL_NODE_ID>38</STILL_NODE_ID>
				<BLURRED_NODE_ID>44</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Engine_Propeller_Visibility_Simple_Template">
				<ID>2</ID>
				<STILL_NODE_ID>39</STILL_NODE_ID>
				<BLURRED_NODE_ID>45</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Engine_Propeller_Visibility_Simple_Template">
				<ID>3</ID>
				<STILL_NODE_ID>40</STILL_NODE_ID>
				<BLURRED_NODE_ID>46</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Engine_Propeller_Visibility_Simple_Template">
				<ID>4</ID>
				<STILL_NODE_ID>41</STILL_NODE_ID>
				<BLURRED_NODE_ID>47</BLURRED_NODE_ID>
			</UseTemplate>
		</Component>
	</Component>

	<Component ID="Flaps">
		<UseTemplate Name="AIG_Flaps">
			<ANIM_LENGTH>200</ANIM_LENGTH>
			<FLAP_TO_DEGREES>50</FLAP_TO_DEGREES>
			<FLAP1_SPEED>160</FLAP1_SPEED>
			<FLAP1_DEGREES>100</FLAP1_DEGREES>
			<FLAP2_SPEED>185</FLAP2_SPEED>
			<FLAP2_DEGREES>50</FLAP2_DEGREES>
			<FLAP3_SPEED>200</FLAP3_SPEED>
			<FLAP3_DEGREES>30</FLAP3_DEGREES>
		</UseTemplate>
	</Component>

	<Component ID="Light_Visibility">
		<Component ID="NoseGear_Landing_Lights">
			<UseTemplate Name="AIG_Landinglight_NoseGear_Visibility">
				<NODE_ID>landing_gear</NODE_ID>
			</UseTemplate>
		</Component>
	</Component>
		
	<Component ID="Effects">
		<UseTemplate Name="AIG_TwinEngineTurboprop_Effects"/>
		<UseTemplate Name="AIG_LandingFX"/>
	</Component>

</Template>

</ModelBehaviors>