<Template Name="KCAI_Gulfstream_G650_Common">

	<Component ID="Landing_Gear">
		<UseTemplate Name="AIG_Gear_Center_Template"/>
		<UseTemplate Name="AIG_Gear_Left_Template"/>
		<UseTemplate Name="AIG_Gear_Right_Template"/>
	</Component>
	
	<Component ID="Wheels">
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>c_tire_still_key</ANIM_NAME>
			<WHEEL_ID>0</WHEEL_ID>
			<RATE>10</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>c_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>0</WHEEL_ID>
			<RATE>10</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>l_tire_still_key</ANIM_NAME>
			<WHEEL_ID>1</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>l_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>1</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>r_tire_still_key</ANIM_NAME>
			<WHEEL_ID>2</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>r_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>2</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
	</Component>

	<Component ID="Wheel_Visibility">
		<Component ID="C_Tire">
			<Parameters Type="Override">
				<WHEEL_ID>0</WHEEL_ID>
			</Parameters>
			<UseTemplate Name="AIG_Tire_Visibility_Template">
				<ID>1</ID>
				<STILL_NODE_ID>62_anim_c_tire_still_key</STILL_NODE_ID>
				<BLURRED_NODE_ID>64_anim_c_tire_blurred_key</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Tire_Visibility_Template">
				<ID>2</ID>
				<STILL_NODE_ID>63_anim_c_tire_still_key</STILL_NODE_ID>
				<BLURRED_NODE_ID>65_anim_c_tire_blurred_key</BLURRED_NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="L_Tire">
			<Parameters Type="Override">
				<WHEEL_ID>1</WHEEL_ID>
			</Parameters>
			<UseTemplate Name="AIG_Tire_Visibility_Template">
				<ID>1</ID>
				<STILL_NODE_ID>72_anim_l_tire_still_key</STILL_NODE_ID>
				<BLURRED_NODE_ID>75_anim_l_tire_blurred_key</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Tire_Visibility_Template">
				<ID>2</ID>
				<STILL_NODE_ID>73_anim_l_tire_still_key</STILL_NODE_ID>
				<BLURRED_NODE_ID>76_anim_l_tire_blurred_key</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Tire_Visibility_Template">
				<ID>3</ID>
				<STILL_NODE_ID>74_anim_l_tire_still_key</STILL_NODE_ID>
				<BLURRED_NODE_ID>77_anim_l_tire_blurred_key</BLURRED_NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="R_Tire">
			<Parameters Type="Override">
				<WHEEL_ID>1</WHEEL_ID>
			</Parameters>
			<UseTemplate Name="AIG_Tire_Visibility_Template">
				<ID>1</ID>
				<STILL_NODE_ID>90_anim_r_tire_still_key</STILL_NODE_ID>
				<BLURRED_NODE_ID>93_anim_r_tire_blurred_key</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Tire_Visibility_Template">
				<ID>2</ID>
				<STILL_NODE_ID>91_anim_r_tire_still_key</STILL_NODE_ID>
				<BLURRED_NODE_ID>94_anim_r_tire_blurred_key</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Tire_Visibility_Template">
				<ID>3</ID>
				<STILL_NODE_ID>92_anim_r_tire_still_key</STILL_NODE_ID>
				<BLURRED_NODE_ID>95_anim_r_tire_blurred_key</BLURRED_NODE_ID>
			</UseTemplate>
		</Component>
	</Component>
	
	<Component ID="Nosewheel_Steering">
		<UseTemplate Name="AIG_Nosewheel_Steering">
			<ANIM_NAME>custom_anim_C_WHEEL_LR</ANIM_NAME>
			<ANIM_SHARPNESS>-20</ANIM_SHARPNESS>
		</UseTemplate>
	</Component>
	
	<Component ID="Spoiler_L">
		<UseTemplate Name="AIG_Spoiler_L_key"/>
