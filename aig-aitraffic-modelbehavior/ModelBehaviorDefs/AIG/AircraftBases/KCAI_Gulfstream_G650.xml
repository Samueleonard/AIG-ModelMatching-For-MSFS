				<STILL_NODE_ID>4</STILL_NODE_ID>
				<SLOW_NODE_ID>48</SLOW_NODE_ID>
				<BLURRED_NODE_ID>49</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Engine_Jet_Visibility_Template">
				<ID>2</ID>
				<STILL_NODE_ID>5</STILL_NODE_ID>
				<SLOW_NODE_ID>52</SLOW_NODE_ID>
				<BLURRED_NODE_ID>53</BLURRED_NODE_ID>
			</UseTemplate>
		</Component>
	</Component>
	
	<Component ID="Wingflex">
		<UseTemplate Name="AIG_Wingflex">
			<ANIM_NAME>AI_Wingflex</ANIM_NAME>
			<WING_ID>1</WING_ID>
			<INTERVAL1>1.8</INTERVAL1>
			<INTERVAL2>2.4</INTERVAL2>
		</UseTemplate>
	</Component>

	<Component ID="Flaps">
		<UseTemplate Name="AIG_Flaps">
			<FLAP_TO_DEGREES>38</FLAP_TO_DEGREES>
			<FLAP1_SPEED>160</FLAP1_SPEED>
			<FLAP1_DEGREES>100</FLAP1_DEGREES>
			<FLAP2_SPEED>186</FLAP2_SPEED>
			<FLAP2_DEGREES>70</FLAP2_DEGREES>
			<FLAP3_SPEED>196</FLAP3_SPEED>
			<FLAP3_DEGREES>38</FLAP3_DEGREES>
			<FLAP4_SPEED>215</FLAP4_SPEED>
			<FLAP4_DEGREES>28</FLAP4_DEGREES>
		</UseTemplate>
    </Component>
	
	<Component ID="Light_Visibility">
		<Component ID="Logo_Lights">
			<UseTemplate Name="AIG_Logolight_Visibility">
				<NODE_ID>logo_l</NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Logolight_Visibility">
				<NODE_ID>logo_r</NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="NoseGear_Landing_Lights">
			<UseTemplate Name="AIG_Landinglight_NoseGear_Visibility">
