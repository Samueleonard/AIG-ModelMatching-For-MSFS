	</Component>
	
	<Component ID="Reverse_Thrust">
		<UseTemplate Name="AIG_Reverse_Thrust"/>
	</Component>
	
	<Component ID="Engines">
		<UseTemplate Name="AIG_Engine_Jet">
			<ANIM_NAME>engine0</ANIM_NAME>
			<ENGINE_ID>1</ENGINE_ID>
		</UseTemplate>
		<UseTemplate Name="AIG_Engine_Jet">
			<ANIM_NAME>engine1</ANIM_NAME>
			<ENGINE_ID>2</ENGINE_ID>
		</UseTemplate>
	</Component>

	<Component ID="Engine_Visibility">
		<Component ID="Engine1">
			<Parameters Type="Override">
				<ENGINE_ID>1</ENGINE_ID>
			</Parameters>
			<UseTemplate Name="AIG_Engine_Jet_Visibility_Template">
				<ID>1</ID>
				<STILL_NODE_ID>2</STILL_NODE_ID>
				<SLOW_NODE_ID>33</SLOW_NODE_ID>
				<BLURRED_NODE_ID>34</BLURRED_NODE_ID>
			</UseTemplate>
			<UseTemplate Name="AIG_Engine_Jet_Visibility_Template">
				<ID>2</ID>
				<STILL_NODE_ID>3</STILL_NODE_ID>
				<SLOW_NODE_ID>36</SLOW_NODE_ID>
				<BLURRED_NODE_ID>37</BLURRED_NODE_ID>
