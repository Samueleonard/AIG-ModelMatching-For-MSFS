<ModelBehaviors>
	<Include ModelBehaviorFile="AIG\Common.xml"/>

<Template Name="SBAI_Beechcraft_Premier1A_Common">

	<Component ID="Lights">
		<UseTemplate Name="AIG_LightsOverride"/>
	</Component>

	<Component ID="Landing_Gear">
		<UseTemplate Name="AIG_Gear_Center_Template"/>
		<UseTemplate Name="AIG_Gear_Left_Template"/>
		<UseTemplate Name="AIG_Gear_Right_Template"/>
	</Component>

	<Component ID="Wheels">
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>c_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>0</WHEEL_ID>
			<RATE>10</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>l_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>1</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>r_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>2</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
	</Component>
	
	<Component ID="Nosewheel_Steering">
		<UseTemplate Name="AIG_Nosewheel_Steering">
			<ANIM_NAME>custom_anim_C_WHEEL_LR</ANIM_NAME>
			<ANIM_SHARPNESS>25</ANIM_SHARPNESS>
		</UseTemplate>
	</Component>
	
	<Component ID="Engines">
		<UseTemplate Name="AIG_Engine_Jet">
			<ANIM_NAME>engine0</ANIM_NAME>
			<ENGINE_ID>1</ENGINE_ID>
		</UseTemplate>
		<UseTemplate Name="AIG_Engine_Jet">
			<ANIM_NAME>engine1</ANIM_NAME>
			<ENGINE_ID>2</ENGINE_ID>
		</UseTemplate>
	</Component>

	<Component ID="Engine_Visibility">
		<Component ID="Engine1">
			<Parameters Type="Override">
				<ENGINE_ID>1</ENGINE_ID>
				<STILL_NODE_ID_COUNT>4</STILL_NODE_ID_COUNT>
				<BLURRED_NODE_ID_COUNT>4</BLURRED_NODE_ID_COUNT>
			</Parameters>
			<UseTemplate Name="AIG_Engine_Jet_Visibility_Simple_Template">
				<ID>1</ID>
				<STILL_NODE_ID>149</STILL_NODE_ID>
				<STILL_2_NODE_ID>150</STILL_2_NODE_ID>
				<STILL_3_NODE_ID>151</STILL_3_NODE_ID>
				<STILL_4_NODE_ID>152</STILL_4_NODE_ID>
				<BLURRED_NODE_ID>192</BLURRED_NODE_ID>
				<BLURRED_2_NODE_ID>194</BLURRED_2_NODE_ID>
				<BLURRED_3_NODE_ID>196</BLURRED_3_NODE_ID>
				<BLURRED_4_NODE_ID>198</BLURRED_4_NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="Engine2">
			<Parameters Type="Override">
				<ENGINE_ID>2</ENGINE_ID>
				<STILL_NODE_ID_COUNT>4</STILL_NODE_ID_COUNT>
				<BLURRED_NODE_ID_COUNT>4</BLURRED_NODE_ID_COUNT>
			</Parameters>
			<UseTemplate Name="AIG_Engine_Jet_Visibility_Simple_Template">
				<ID>1</ID>
				<STILL_NODE_ID>154</STILL_NODE_ID>
				<STILL_2_NODE_ID>155</STILL_2_NODE_ID>
				<STILL_3_NODE_ID>156</STILL_3_NODE_ID>
				<STILL_4_NODE_ID>157</STILL_4_NODE_ID>
				<BLURRED_NODE_ID>193</BLURRED_NODE_ID>
				<BLURRED_2_NODE_ID>195</BLURRED_2_NODE_ID>
				<BLURRED_3_NODE_ID>197</BLURRED_3_NODE_ID>
				<BLURRED_4_NODE_ID>199</BLURRED_4_NODE_ID>
			</UseTemplate>
		</Component>
	</Component>

	<Component ID="Flaps">
		<UseTemplate Name="AIG_Flaps">
			<FLAP_TO_DEGREES>25</FLAP_TO_DEGREES>
			<FLAP1_SPEED>140</FLAP1_SPEED>
			<FLAP1_DEGREES>100</FLAP1_DEGREES>
			<FLAP2_SPEED>160</FLAP2_SPEED>
			<FLAP2_DEGREES>50</FLAP2_DEGREES>
			<FLAP3_SPEED>180</FLAP3_SPEED>
			<FLAP3_DEGREES>25</FLAP3_DEGREES>
		</UseTemplate>
	</Component>
	
	<Component ID="Effects">
		<UseTemplate Name="AIG_TwinEngineJet_Effects"/>
		<UseTemplate Name="AIG_LandingFX">
			<Medium>True</Medium>
		</UseTemplate>
	</Component>

</Template>

</ModelBehaviors>