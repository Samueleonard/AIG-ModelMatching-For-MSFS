<ModelBehaviors>
	<Include ModelBehaviorFile="AIG\Common.xml"/>

<Template Name="HTAI_deHavilland_DHC6_Common">

	<Component ID="Lights">
		<UseTemplate Name="AIG_LightsOverride"/>
	</Component>

	<Component ID="Wheels">
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>c_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>0</WHEEL_ID>
			<RATE>10</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>l_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>1</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>r_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>2</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
	</Component>
	
	<Component ID="Nosewheel_Steering">
		<UseTemplate Name="AIG_Nosewheel_Steering">
			<ANIM_NAME>custom_anim_C_WHEEL_LR</ANIM_NAME>
			<ANIM_SHARPNESS>50</ANIM_SHARPNESS>
		</UseTemplate>
	</Component>
	
	<Component ID="Engines">
		<UseTemplate Name="AIG_Engine_Propeller">
			<ANIM_NAME>engine0</ANIM_NAME>
			<ENGINE_ID>1</ENGINE_ID>
		</UseTemplate>
		<UseTemplate Name="AIG_Engine_Propeller">
			<ANIM_NAME>engine1</ANIM_NAME>
			<ENGINE_ID>2</ENGINE_ID>
		</UseTemplate>
	</Component>

	<Component ID="Engine_Visibility">
		<Component ID="Engine1">
			<Parameters Type="Override">
				<ENGINE_ID>1</ENGINE_ID>
				<STILL_NODE_ID_COUNT>4</STILL_NODE_ID_COUNT>
				<BLURRED_NODE_ID_COUNT>4</BLURRED_NODE_ID_COUNT>
			</Parameters>
			<UseTemplate Name="AIG_Engine_Propeller_Visibility_Simple_Template">
				<ID>1</ID>
				<STILL_NODE_ID>56</STILL_NODE_ID>
				<STILL_2_NODE_ID>57</STILL_2_NODE_ID>
				<STILL_3_NODE_ID>58</STILL_3_NODE_ID>
				<STILL_4_NODE_ID>59</STILL_4_NODE_ID>
				<BLURRED_NODE_ID>26</BLURRED_NODE_ID>
				<BLURRED_2_NODE_ID>27</BLURRED_2_NODE_ID>
				<BLURRED_3_NODE_ID>28</BLURRED_3_NODE_ID>
				<BLURRED_4_NODE_ID>29</BLURRED_4_NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="Engine2">
			<Parameters Type="Override">
				<ENGINE_ID>2</ENGINE_ID>
				<STILL_NODE_ID_COUNT>4</STILL_NODE_ID_COUNT>
				<BLURRED_NODE_ID_COUNT>4</BLURRED_NODE_ID_COUNT>
			</Parameters>
			<UseTemplate Name="AIG_Engine_Propeller_Visibility_Simple_Template">
				<ID>1</ID>
				<STILL_NODE_ID>50</STILL_NODE_ID>
				<STILL_2_NODE_ID>51</STILL_2_NODE_ID>
				<STILL_3_NODE_ID>52</STILL_3_NODE_ID>
				<STILL_4_NODE_ID>53</STILL_4_NODE_ID>
				<BLURRED_NODE_ID>32</BLURRED_NODE_ID>
				<BLURRED_2_NODE_ID>33</BLURRED_2_NODE_ID>
				<BLURRED_3_NODE_ID>34</BLURRED_3_NODE_ID>
				<BLURRED_4_NODE_ID>35</BLURRED_4_NODE_ID>
			</UseTemplate>
		</Component>
	</Component>

	<Component ID="Flaps">
		<UseTemplate Name="AIG_Flaps">
			<ANIM_LENGTH>200</ANIM_LENGTH>
			<FLAP_TO_DEGREES>50</FLAP_TO_DEGREES>
			<FLAP1_SPEED>80</FLAP1_SPEED>
			<FLAP1_DEGREES>100</FLAP1_DEGREES>
			<FLAP2_SPEED>95</FLAP2_SPEED>
			<FLAP2_DEGREES>50</FLAP2_DEGREES>
			<FLAP3_SPEED>110</FLAP3_SPEED>
			<FLAP3_DEGREES>30</FLAP3_DEGREES>
		</UseTemplate>
	</Component>
	
	<Component ID="Effects">
		<UseTemplate Name="AIG_TwinEngineTurboprop_Effects"/>
		<UseTemplate Name="AIG_LandingFX">
			<Small>True</Small>
		</UseTemplate>
	</Component>
	
	<Component ID="Fuselage_DimCabin">
		<UseTemplate Name="AIG_Fuselage_DimCabin">
			<NODE_ID>node13</NODE_ID>
			<TIME_START>26100</TIME_START>
			<TIME_END>80100</TIME_END>
		</UseTemplate>
	</Component>

</Template>

</ModelBehaviors>