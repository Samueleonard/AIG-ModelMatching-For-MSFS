<ModelBehaviors>
	<Include ModelBehaviorFile="AIG\Common.xml"/>

<Template Name="AIA_Canadair_CL215_Common">

	<Component ID="Lights">
		<UseTemplate Name="AIG_LightsOverride"/>
	</Component>

	<Component ID="Landing_Gear">
		<UseTemplate Name="AIG_Gear_Center_Template">
			<RETRACT_TIME>6.0</RETRACT_TIME>
		</UseTemplate>
		<UseTemplate Name="AIG_Gear_Left_Template">
			<RETRACT_TIME>8.3</RETRACT_TIME>
		</UseTemplate>
		<UseTemplate Name="AIG_Gear_Right_Template">
			<RETRACT_TIME>7.1</RETRACT_TIME>
		</UseTemplate>
	</Component>

	<Component ID="Wheels">
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>c_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>0</WHEEL_ID>
			<RATE>10</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>l_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>1</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
		<UseTemplate Name="AIG_Wheel_Animation">
			<ANIM_NAME>r_tire_blurred_key</ANIM_NAME>
			<WHEEL_ID>2</WHEEL_ID>
			<RATE>7</RATE>
		</UseTemplate>
	</Component>
	
	<Component ID="Nosewheel_Steering">
		<UseTemplate Name="AIG_Nosewheel_Steering">
			<ANIM_NAME>custom_anim_C_WHEEL_LR</ANIM_NAME>
			<ANIM_SHARPNESS>30</ANIM_SHARPNESS>
		</UseTemplate>
	</Component>

	<Component ID="Engines">
		<UseTemplate Name="AIG_Engine_Propeller">
			<ANIM_NAME>engine0</ANIM_NAME>
			<ENGINE_ID>1</ENGINE_ID>
		</UseTemplate>
		<UseTemplate Name="AIG_Engine_Propeller">
			<ANIM_NAME>engine1</ANIM_NAME>
			<ENGINE_ID>2</ENGINE_ID>
		</UseTemplate>
	</Component>

	<Component ID="Engine_Visibility">
		<Component ID="Engine1">
			<Parameters Type="Override">
				<ENGINE_ID>1</ENGINE_ID>
				<STILL_NODE_ID_COUNT>2</STILL_NODE_ID_COUNT>
				<SLOW_NODE_ID_COUNT>3</SLOW_NODE_ID_COUNT>
				<BLURRED_NODE_ID_COUNT>3</BLURRED_NODE_ID_COUNT>
			</Parameters>
			<UseTemplate Name="AIG_Engine_Propeller_Visibility_Template">
				<ID>1</ID>
				<STILL_NODE_ID>75</STILL_NODE_ID>
				<STILL_2_NODE_ID>76</STILL_2_NODE_ID>
				<SLOW_NODE_ID>63</SLOW_NODE_ID>
				<SLOW_2_NODE_ID>64</SLOW_2_NODE_ID>
				<SLOW_3_NODE_ID>65</SLOW_3_NODE_ID>
				<BLURRED_NODE_ID>69</BLURRED_NODE_ID>
				<BLURRED_2_NODE_ID>70</BLURRED_2_NODE_ID>
				<BLURRED_3_NODE_ID>71</BLURRED_3_NODE_ID>
			</UseTemplate>
		</Component>
		<Component ID="Engine2">
			<Parameters Type="Override">
				<ENGINE_ID>2</ENGINE_ID>
				<STILL_NODE_ID_COUNT>2</STILL_NODE_ID_COUNT>
				<SLOW_NODE_ID_COUNT>3</SLOW_NODE_ID_COUNT>
				<BLURRED_NODE_ID_COUNT>3</BLURRED_NODE_ID_COUNT>
			</Parameters>
			<UseTemplate Name="AIG_Engine_Propeller_Visibility_Template">
				<ID>1</ID>
				<STILL_NODE_ID>77</STILL_NODE_ID>
				<STILL_2_NODE_ID>78</STILL_2_NODE_ID>
				<SLOW_NODE_ID>66</SLOW_NODE_ID>
				<SLOW_2_NODE_ID>67</SLOW_2_NODE_ID>
				<SLOW_3_NODE_ID>68</SLOW_3_NODE_ID>
				<BLURRED_NODE_ID>72</BLURRED_NODE_ID>
				<BLURRED_2_NODE_ID>73</BLURRED_2_NODE_ID>
				<BLURRED_3_NODE_ID>74</BLURRED_3_NODE_ID>
			</UseTemplate>
		</Component>
	</Component>

	<Component ID="FlapsSlats">
		<UseTemplate Name="AIG_Flaps">
			<FLAP_TO_DEGREES>50</FLAP_TO_DEGREES>
			<FLAP1_SPEED>85</FLAP1_SPEED>
			<FLAP1_DEGREES>100</FLAP1_DEGREES>
			<FLAP2_SPEED>100</FLAP2_SPEED>
			<FLAP2_DEGREES>50</FLAP2_DEGREES>
			<FLAP3_SPEED>110</FLAP3_SPEED>
			<FLAP3_DEGREES>30</FLAP3_DEGREES>
		</UseTemplate>
	</Component>

	<Component ID="Effects">
		<UseTemplate Name="AIG_TwinEnginePiston_Effects"/>
		<UseTemplate Name="AIG_LandingFX">
			<Medium>True</Medium>
		</UseTemplate>
	</Component>

</Template>

</ModelBehaviors>