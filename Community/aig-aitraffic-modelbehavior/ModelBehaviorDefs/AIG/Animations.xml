	<Template Name="AIG_Engine_Jet">
		<Parameters Type="Default">
			<ANIM_NAME>engine0</ANIM_NAME>
			<ENGINE_ID>0</ENGINE_ID>
		</Parameters>
			<UseTemplate Name="ASOBO_GT_Anim_Code">
				<ANIM_CODE>(A:AMBIENT WIND VELOCITY, knots) 6 &gt; (A:TURB ENG N1:#ENGINE_ID#, Percent) 6.25 &lt; and if{ (A:AMBIENT WIND VELOCITY, knots) 0.02 * } 
						els{ (A:TURB ENG N1:#ENGINE_ID#, Percent) 6.25 &gt; if{ 0.01 6 3500 (A:TURB ENG N1:#ENGINE_ID#, Percent) (A:ANIMATION DELTA TIME, seconds) * * * * } 
						els{ 0 } }</ANIM_CODE>
