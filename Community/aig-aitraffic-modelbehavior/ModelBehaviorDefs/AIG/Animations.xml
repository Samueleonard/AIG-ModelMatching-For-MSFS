<!-- General Animations -->


	<Template Name="AIG_Nosewheel_Steering">
		<Parameters Type="Default">
			<ANIM_NAME>custom_anim_C_WHEEL_LR</ANIM_NAME>
			<ANIM_LENGTH>200</ANIM_LENGTH>
			<ANIM_SHARPNESS>0</ANIM_SHARPNESS>
			<ANIM_CENTER>150</ANIM_CENTER>
		</Parameters>
			<UseTemplate Name="ASOBO_GT_Anim_Code">
				<ANIM_CODE>(A:SIM ON GROUND, bool) 0 == if{ #ANIM_CENTER# } els{ (A:ROTATION VELOCITY BODY Y, Feet) #ANIM_SHARPNESS# * #ANIM_CENTER# + 100 max 200 min }</ANIM_CODE>
				<ANIM_LENGTH>200</ANIM_LENGTH>
				<ANIM_LAG>30</ANIM_LAG>
